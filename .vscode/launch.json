    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/bin/www",
            "env": {
                "SQLITE_DB_PATH": "${workspaceFolder}/data/db/",
                "MAP_CACHE_PATH": "${workspaceFolder}/data/map/",
                "STATS_PATH": "${workspaceFolder}/data/stats/"
            }
        }
    ]
}